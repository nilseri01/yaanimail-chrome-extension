{
  "name": "YaaniMail Chrome Extension",
  "description": "YaaniMail Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["http://api.yaanimail.com/*", "storage", "notifications"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo-minimized.png",
    "48": "logo-minimized.png",
    "128": "logo-minimized.png"
  }
}
