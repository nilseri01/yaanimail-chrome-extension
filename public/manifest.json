{
  "name": "YaaniMail Chrome Extension",
  "description": "YaaniMail Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "keywords": ["react", "browser extension", "chrome extension", "yaanimail"],
  "permissions": [
    "http://api.yaanimail.com/*",
    "storage",
    "notifications",
    "alarms"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo-minimized.png",
    "48": "logo-minimized.png",
    "128": "logo-minimized.png"
  }
}
